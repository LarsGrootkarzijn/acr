{
    "action_plugins": [
        {
            "active-monitor": {
                "enabled": true
            }
        },
        {
            "event-logger": {
                "enabled": true,
                "event_types": [
                    "state",
                    "song",
                    "loop",
                    "capabilities",
                    "position",
                    "song_information_update"
                ],
                "log_level": "info",
                "only_active": false
            }
        },
        {
            "lastfm": {
                "enabled": true,
                "api_key": "",
                "api_secret": "",
                "scrobble": true
            }
        }
    ],
    "webserver": {
        "enable": true,
        "host": "0.0.0.0",
        "port": 1080,
        "static_routes": [
            {
                "url_path": "/web",
                "directory": "/usr/share/acr/web"
            }
        ]
    },
    "cache": {
        "attribute_cache_path": "/var/acr/cache/attributes",
        "image_cache_path": "/var/acr/cache/images"
    },    "musicbrainz": {
        "enable": true,
        "rate_limit_ms": 1000
    },    
    "theaudiodb": {
        "enable": true,
        "api_key": "", 
        "rate_limit_ms": 500
    },
    "lastfm": {
        "enable": true,
        "api_key": "",
        "api_secret": "",
        "scrobble_enabled": true, // This seems to be a duplicate or alternative, will use "scrobble" as per previous request for consistency
        "now_playing_enabled": true,
        "scrobble": true 
    },
    "general": {
        "security_store": "/var/run/acr/security_store.json",
    },
    "players": [
        {
            "mpd": {
                "enable": true,
                "host": "localhost",
                "port": 6600,
                "load_on_startup": true,
                "artist_separator": [",", "feat. "],
                "enhance_metadata": true
            }
        },
        {
            "raat": {
                "enable": true,
                "metadata_pipe": "/var/run/raat/metadata_pipe",
                "control_pipe": "/var/run/raat/control_pipe",
                "reopen_metadata_pipe": true
            }
        },
        {
            "librespot": {
                "enable": true,
                "event_pipe": "/var/run/librespot/event_pipe",
                "reopen_event_pipe": true
            }
        },
        {
            "lms": {
                "enable": true,
                "server": null,
                "port": 9000,
                "autodiscovery": true,
                "player_name": null,
                "player_mac": [],
                "reconnection_interval": 30,
                "polling_interval": 30
            }
        }
    ]
}