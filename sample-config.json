{
    "action_plugins": [
        {
            "active-monitor": {
                "enabled": true
            }
        }
    ],
    "event_filters": [
        {
            "event-logger": {
                "enabled": true,
                "event_types": [
                    "state",
                    "song",
                    "loop",
                    "capabilities",
                    "position"
                ],
                "log_level": "info",
                "only_active": false
            }
        }
    ],
    "api": {
        "enable": true,
        "host": "0.0.0.0",
        "port": 1080
    },
    "cache": {
        "attribute_cache_path": "cache/attributes",
        "image_cache_path": "cache/images"
    },
    "players": [
        {
            "mpd": {
                "enable": true,
                "host": "localhost",
                "port": 6600,
                "load_on_startup": true
            }
        },
        {
            "raat": {
                "enable": false,
                "metadata_pipe": "tcp://headphone:12100",
                "control_pipe": "tcp://headphone:12101",
                "reopen_metadata_pipe": true
            }
        },
        {
            "librespot": {
                "enable": false,
                "event_pipe": "tcp://headphone:12102",
                "reopen_event_pipe": true
            }
        }
    ]
}