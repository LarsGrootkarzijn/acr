{
  "level": "info",
  "target": "stdout",
  "timestamps": true,
  "colors": true,
  "include_module_path": true,
  "include_line_numbers": false,
  "subsystems": {
    "players": "info",
    "cache": "warn",
    "network": "error",
    "deps": "warn",
    "database": "warn",
    "http": "warn",

    "_comment_specific": "Examples of specific module targeting:",
    
    "audiocontrol::helpers::theaudiodb": "info",
    "audiocontrol::helpers::lastfm": "info",
    "audiocontrol::helpers::musicbrainz": "info",
    "audiocontrol::helpers::spotify": "info",
    "audiocontrol::helpers::settingsdb": "info",
    "audiocontrol::helpers::global_volume": "warn",
    "audiocontrol::helpers::lyrics": "info",
    "audiocontrol::helpers::songtitlesplitter": "debug",
    "audiocontrol::helpers::songsplitmanager": "debug"
    
  },
  "env_overrides": {
    "RUST_BACKTRACE": "1"
  }
}
