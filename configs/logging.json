{
  "level": "info",
  "target": "stdout",
  "timestamps": true,
  "colors": true,
  "include_module_path": true,
  "include_line_numbers": false,
  "subsystems": {
    "players": "info",
    "cache": "warn",
    "network": "error",
    "deps": "warn",

    "audiocontrol::helpers::theaudiodb": "info",
    "audiocontrol::helpers::lastfm": "info",
    "audiocontrol::helpers::musicbrainz": "info",
    
    "_comment_specific": "Examples of specific module targeting:",
    "_audiocontrol::players::librespot::librespot": "debug",
    "_audiocontrol::api::server": "info",
    "database": "warn",
    "http": "warn"
  },
  "env_overrides": {
    "RUST_BACKTRACE": "1"
  }
}
