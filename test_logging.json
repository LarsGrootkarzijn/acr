{
  "level": "info",
  "target": "stdout",
  "timestamps": true,
  "colors": true,
  "include_module_path": true,
  "include_line_numbers": false,
  "subsystems": {
    "players": "info",
    "deps": "warn",
    "_comment": "Examples of specific module targeting (remove _ to activate):",
    "_audiocontrol::players::librespot::librespot": "debug"
  },
  "env_overrides": {
    "RUST_BACKTRACE": "1"
  }
}
